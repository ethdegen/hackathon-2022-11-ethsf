{
    "customizations": {
        "vscode": {
            "extensions": ["eamodio.gitlens", "esbenp.prettier-vscode", "ms-python.python"]
        }
    },
    "dockerComposeFile": ["./docker-compose.yml"],
    "mounts": [{
        "source": "~/.ssh",
        "target": "/root/.ssh",
        "type": "bind"
    }],
    "service": "workspace",
    "shutdownAction": "stopCompose",
    "workspaceFolder": "/workspace"
}
